        "fontStyle": "",
        "foreground": "#E91E63"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#9EE34F"
      }
