      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#60cf30"
      }
    },
    {
      "name": "diff: range",
      "scope": [
        "meta.diff",
        "meta.diff.range"
